{
  "$schema": "https://raw.githubusercontent.com/oxc-project/oxc/main/npm/oxlint/configuration_schema.json",
  "ignorePatterns": ["dist"],
  "categories": {
    "correctness": "error",
    "suspicious": "warn",
    "perf": "warn"
  },
  "globals": {
    "chrome": "readonly",
    "JSONEditor": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
